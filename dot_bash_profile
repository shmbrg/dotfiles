export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
if command -v pyenv 1>/dev/null 2>&1; then
  eval "$(pyenv init -)"
fi

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/humberg/Downloads/google-cloud-sdk 2/path.bash.inc' ]; then . '/Users/humberg/Downloads/google-cloud-sdk 2/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/humberg/Downloads/google-cloud-sdk 2/completion.bash.inc' ]; then . '/Users/humberg/Downloads/google-cloud-sdk 2/completion.bash.inc'; fi

. /usr/local/opt/asdf/asdf.sh

. /usr/local/opt/asdf/etc/bash_completion.d/asdf.bash

export VAULT_ADDR=https://vault.ops.zeit.de
